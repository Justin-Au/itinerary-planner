{
  "name": "itinerary-planner",
  "version": "1.0.0",
  "description": "A comprehensive travel itinerary planning application with web and mobile interfaces",
  "private": true,
  "scripts": {
    "dev:backend": "cd app/backend && pnpm run dev",
    "dev:web": "cd app/frontend-web && pnpm start",
    "dev:mobile": "cd app/frontend-mobile && pnpm start",
    "build:backend": "cd app/backend && pnpm run build",
    "build:web": "cd app/frontend-web && pnpm run build",
    "test:backend": "cd app/backend && pnpm test",
    "test:web": "cd app/frontend-web && pnpm test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format"
  },
  "keywords": [
    "itinerary",
    "travel",
    "planning",
    "typescript",
    "react",
    "react-native",
    "express"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
